# ====== Wajib ======
TOKEN=your_discord_token_here

# ====== Opsional (aktifkan sesuai kebutuhan) ======
# Prefix teks tambahan selain mention (<@id>) untuk menjalankan command.
TEXT_PREFIX=!

# Level logging Pino (trace|debug|info|warn|error|fatal). Default debug di non-production.
LOG_LEVEL=info

# Role yang berhak menjalankan command saat bot berada di guild.
ADMIN_ROLE_ID=1413068480021921802

# Channel ID untuk fitur auto emoji reactor.
EMOJI_CHANNEL_ID=1413076616787001444

# URL webhook untuk command `webhook`.
WEBHOOK_URL=https://discord.com/api/webhooks/xxxxxxxxx/yyyyyyyyy

# Atur true/1 untuk mengaktifkan rotasi status otomatis di event ready.
AUTO_STATUS_ROTATOR=false

# Voice channel yang akan dijoin ketika AUTO_STATUS_ROTATOR aktif.
VOICE_CHANNEL_ID=1416782872081207398

# Channel tujuan scheduler meme harian (06:00 WIB).
DAILY_MEME_CHANNEL_ID=992086043392225421

# Override API meme (opsional). Default: https://candaan-api.vercel.app/api/image/random
DAILY_MEME_API_URL=
